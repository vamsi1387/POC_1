containerid=`docker container ls -n 1 -q`
echo $containerid
da=`docker diff $containerid`
image=`docker ps --format='{{.Image}}' -n 1`
USERNAME=akhilra

if [ -z "$da" ]
then
      echo "No change in image"
else`
      ./test.sh //script for version
      VERSION=`cat VERSION`
      docker commit $containerid $USERNAME/$image:$VERSION
fi
